{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "env": {
      "DATABASE_URL": "sqlite://./database.sqlite"
    },
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/api"
      }
    ]
  }